<tryton>
    <data grouped="1">
        <record model="ir.message" id="warn_line_without_destination">
            <field name="text">You tried to open related destination lines, but the current line does not have a destination.</field>
        </record>
        <record model="ir.message" id="warn_line_without_source">
            <field name="text">You tried to open related source lines, but the current line does not have a source.</field>
        </record>
        <record model="ir.message" id="title_destination_relate">
            <field name="text">Lines with destination "$(destination)"</field>
        </record>
        <record model="ir.message" id="title_source_relate">
            <field name="text">Lines with source "$(source)"</field>
        </record>
    </data>
</tryton>
